{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "consumers"
  ],
  "properties": {
    "schema_version": {
      "type": "integer",
      "const": 1
    },
    "consumers": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "id",
          "kind",
          "owner",
          "dataset_ids",
          "interface"
        ],
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "kind": {
            "type": "string",
            "minLength": 1
          },
          "owner": {
            "type": "string",
            "minLength": 1
          },
          "dataset_ids": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "string",
              "minLength": 1
            }
          },
          "interface": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  },
  "$id": "ops/schema/datasets/consumer-list.schema.json"
}

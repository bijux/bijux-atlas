{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["schema_version", "source_manifest", "source_lock", "dataset_ids", "missing_dataset_ids", "stale_dataset_ids"],
  "properties": {
    "schema_version": {"type": "integer", "const": 1},
    "source_manifest": {"type": "string", "minLength": 1},
    "source_lock": {"type": "string", "minLength": 1},
    "dataset_ids": {"type": "array", "minItems": 1, "items": {"type": "string"}},
    "missing_dataset_ids": {"type": "array", "items": {"type": "string"}},
    "stale_dataset_ids": {"type": "array", "items": {"type": "string"}}
  }
}

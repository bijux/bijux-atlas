{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "schema_version",
    "render_outputs",
    "determinism"
  ],
  "properties": {
    "schema_version": {
      "type": "integer",
      "const": 1
    },
    "render_outputs": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "determinism": {
      "type": "object",
      "required": [
        "required",
        "hash_algorithm"
      ],
      "properties": {
        "required": {
          "type": "boolean"
        },
        "hash_algorithm": {
          "type": "string",
          "minLength": 1
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "$id": "ops/schema/k8s/render-artifact-index.schema.json"
}

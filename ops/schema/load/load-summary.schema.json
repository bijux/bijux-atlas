{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["schema_version", "suite_manifest", "deterministic_seed", "query_pack", "suites", "scenario_coverage"],
  "properties": {
    "schema_version": { "type": "integer", "const": 1 },
    "suite_manifest": { "type": "string", "minLength": 1 },
    "deterministic_seed": { "type": "integer", "minimum": 1 },
    "query_pack": { "type": "string", "minLength": 1 },
    "suites": { "type": "array", "items": { "type": "string" }, "minItems": 1 },
    "scenario_coverage": {
      "type": "object",
      "additionalProperties": false,
      "required": ["covered", "missing"],
      "properties": {
        "covered": { "type": "array", "items": { "type": "string" } },
        "missing": { "type": "array", "items": { "type": "string" } }
      }
    }
  }
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["schema_version", "generated_by", "status", "entries"],
  "properties": {
    "schema_version": {"type": "integer", "const": 1},
    "generated_by": {"type": "string", "minLength": 1},
    "status": {"type": "string", "enum": ["pass", "fail"]},
    "entries": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["artifact", "producer", "regenerate", "lifecycle"],
        "properties": {
          "artifact": {"type": "string", "minLength": 1},
          "producer": {"type": "string", "minLength": 1},
          "regenerate": {"type": "string", "minLength": 1},
          "lifecycle": {"type": "string", "enum": ["curated_evidence", "domain_derived", "transient_runtime"]},
          "generator_version": {"type": "string", "minLength": 1}
        }
      }
    },
    "notes": {"type": "array", "items": {"type": "string", "minLength": 1}}
  }
}

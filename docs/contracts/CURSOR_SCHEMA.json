{
  "description": "Opaque cursor payload contract for v1 pagination.",
  "error_reason_codes": [
    "CURSOR_DATASET_MISMATCH",
    "CURSOR_INVALID",
    "CURSOR_VERSION_UNSUPPORTED"
  ],
  "fields": {
    "cursor_version": {
      "required": true,
      "type": "string",
      "value": "v1"
    },
    "dataset_id": {
      "required": true,
      "type": "string"
    },
    "last_seen": {
      "required": true,
      "type": "object",
      "value": {
        "gene_id": "string",
        "seqid": "string|null",
        "start": "integer|null"
      }
    },
    "sort_key": {
      "required": true,
      "type": "string",
      "values": [
        "gene_id",
        "region"
      ]
    }
  },
  "opaque_encoding": {
    "envelope": "base64url payload + HMAC signature",
    "transport": "token",
    "version_prefix": "v1."
  },
  "schema_version": 1,
  "type": "cursor_schema"
}

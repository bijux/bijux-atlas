{
  "title": "Bijux Atlas Observability",
  "timezone": "utc",
  "panels": [
    {
      "title": "HTTP Request Rate by Route/Status",
      "type": "timeseries",
      "targets": [{"expr": "sum by (route,status) (rate(atlas_http_requests_total[5m]))"}]
    },
    {
      "title": "HTTP p95 Latency by Route",
      "type": "timeseries",
      "targets": [{"expr": "atlas_http_request_latency_p95_seconds"}]
    },
    {
      "title": "SQLite Query p95 by Class",
      "type": "timeseries",
      "targets": [{"expr": "atlas_sqlite_query_latency_p95_seconds"}]
    },
    {
      "title": "Store Open/Download p95",
      "type": "timeseries",
      "targets": [
        {"expr": "atlas_store_open_p95_seconds"},
        {"expr": "atlas_store_download_p95_seconds"}
      ]
    },
    {
      "title": "Dataset Cache Hit/Miss",
      "type": "timeseries",
      "targets": [
        {"expr": "rate(atlas_dataset_hits[5m])"},
        {"expr": "rate(atlas_dataset_misses[5m])"}
      ]
    },
    {
      "title": "Dataset Cache Size",
      "type": "stat",
      "targets": [
        {"expr": "atlas_dataset_count"},
        {"expr": "atlas_dataset_disk_usage_bytes"}
      ]
    }
  ]
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "atlasctl.output-base.v1",
  "type": "object",
  "required": [
    "schema_name",
    "schema_version",
    "tool",
    "ok",
    "errors",
    "warnings",
    "meta",
    "run_id"
  ],
  "properties": {
    "schema_name": {"const": "atlasctl.output-base.v1"},
    "schema_version": {"const": 1},
    "tool": {"const": "atlasctl"},
    "ok": {"type": "boolean"},
    "errors": {"type": "array", "items": {"type": "string"}},
    "warnings": {"type": "array", "items": {"type": "string"}},
    "meta": {"type": "object"},
    "run_id": {"type": "string", "minLength": 1}
  },
  "additionalProperties": true
}

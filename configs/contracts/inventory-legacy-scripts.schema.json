{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Legacy Scripts Inventory",
  "type": "object",
  "required": [
    "kind",
    "files",
    "count"
  ],
  "properties": {
    "kind": {
      "const": "legacy-scripts"
    },
    "files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "count": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false
}

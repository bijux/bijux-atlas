{
  "version": "1",
  "variables": {
    "ATLAS_BASE_URL": {
      "type": "url",
      "default": "http://127.0.0.1:18080"
    },
    "ATLAS_NS": {
      "type": "k8s_name",
      "default": "atlas-e2e"
    },
    "ATLAS_VALUES_FILE": {
      "type": "path",
      "default": "ops/k8s/values/local.yaml",
      "must_exist": true
    },
    "ATLAS_OFFLINE_VALUES_FILE": {
      "type": "path",
      "default": "ops/k8s/values/offline.yaml",
      "must_exist": true
    },
    "ATLAS_PERF_VALUES_FILE": {
      "type": "path",
      "default": "ops/k8s/values/perf.yaml",
      "must_exist": true
    },
    "ATLAS_MULTI_REGISTRY_VALUES_FILE": {
      "type": "path",
      "default": "ops/k8s/values/multi-registry.yaml",
      "must_exist": true
    },
    "ATLAS_INGRESS_VALUES_FILE": {
      "type": "path",
      "default": "ops/k8s/values/ingress.yaml",
      "must_exist": true
    },
    "ATLAS_E2E_NAMESPACE": {
      "type": "k8s_name",
      "default_from": "ATLAS_NS"
    },
    "ATLAS_E2E_VALUES_FILE": {
      "type": "path",
      "default_from": "ATLAS_VALUES_FILE",
      "must_exist": true
    },
    "ATLAS_E2E_RELEASE_NAME": {
      "type": "string",
      "default": "atlas-e2e"
    },
    "ATLAS_RUN_ID": {
      "type": "string",
      "default": "local"
    },
    "ATLAS_E2E_TIMEOUT": {
      "type": "string",
      "default": "180s"
    },
    "ATLAS_E2E_ENABLE_REDIS": {
      "type": "string",
      "default": "0"
    },
    "ATLAS_E2E_ENABLE_OTEL": {
      "type": "string",
      "default": "0"
    },
    "ATLAS_E2E_ENABLE_TOXIPROXY": {
      "type": "string",
      "default": "0"
    },
    "ALLOW_NON_KIND": {
      "type": "string",
      "default": "0"
    },
    "ATLAS_ALLOW_MIN_VIABLE_RESPONSE": {
      "type": "string",
      "default": "1"
    },
    "ATLAS_ALLOW_PRIVATE_STORE_HOSTS": {
      "type": "string",
      "default": "0"
    },
    "OPS_RUN_ID": {
      "type": "string",
      "default": ""
    },
    "OPS_RUN_DIR": {
      "type": "string",
      "default": "artifacts/ops/${OPS_RUN_ID}"
    },
    "RUN_ID": {
      "type": "string",
      "default": ""
    },
    "ARTIFACT_DIR": {
      "type": "string",
      "default": "artifacts/ops/${RUN_ID}"
    },
    "OPS_ENV_SCHEMA": {
      "type": "string",
      "default": "configs/ops/env.schema.json"
    },
    "OPS_RETENTION_DAYS": {
      "type": "integer",
      "default": 7
    },
    "OPS_ERR_PREREQ": {
      "type": "integer",
      "default": 20
    },
    "OPS_ERR_VERSION": {
      "type": "integer",
      "default": 21
    },
    "OPS_ERR_CONFIG": {
      "type": "integer",
      "default": 22
    },
    "OPS_ERR_ARTIFACT": {
      "type": "integer",
      "default": 23
    },
    "OPS_LIB_ROOT": {
      "type": "string",
      "default": "ops/_lib"
    },
    "PROFILE": {
      "type": "string",
      "default": "kind"
    },
    "SUITE": {
      "type": "string",
      "default": "smoke"
    },
    "ATLAS_OBS_PROFILE": {
      "type": "string",
      "default": "compose"
    }
  }
}

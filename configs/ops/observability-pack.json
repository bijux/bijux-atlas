      "offline_supported": true
    },
    "kind": {
      "runtime": "kubernetes",
      "namespace": "atlas-observability",
      "storage_mode_default": "ephemeral",
      "offline_supported": true
    },
    "cluster": {
      "runtime": "kubernetes",
      "namespace": "atlas-observability",
      "storage_mode_default": "ephemeral",
